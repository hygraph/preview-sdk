# Copy this file to .env.local and fill in your Hygraph endpoint
# Get this from your Hygraph project settings -> API Access -> Content API

HYGRAPH_ENDPOINT=https://eu-central-1.cdn.hygraph.com/content/YOUR_PROJECT_ID/master
HYGRAPH_TOKEN=YOUR_PAT_HERE

# Optional: Enable debug mode (already enabled in development)
INSPECTOR_DEBUG=true
HYGRAPH_STUDIO_URL=Optional Studio URL